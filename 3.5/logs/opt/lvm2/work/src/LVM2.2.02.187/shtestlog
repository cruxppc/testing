| [ 0:01] get_image_pvs() {
| [ 0:01] 	local d
| [ 0:01] 	local images=()
| [ 0:01] 
| [ 0:01] 	images=( $(dmsetup ls | grep "${1}-${2}_.image_.*" | cut -f1 | sed -e s:-:/:) )
| [ 0:01] 	lvs --noheadings -a -o devices "${images[@]}" | sed s/\(.\)//
| [ 0:01] }
