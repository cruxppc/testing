| [ 0:11] ## Line: 22 	 aux prepare_vg 8
| [ 0:11] ## Line: 23 	 get_devs
| [ 0:11] ## Line: 24 	 
| [ 0:11] ## Line: 25 	 _sync() {
| [ 0:11] ## Line: 26 	 	aux enable_dev "${DEVICES[@]}"
| [ 0:11] ## Line: 27 	 
| [ 0:11] ## Line: 28 	 	aux wait_for_sync $vg $lv1
| [ 0:11] ## Line: 29 	 	test "$#" -eq 0 || check raid_leg_status $vg $lv1 "$@"
| [ 0:11] ## Line: 30 	 
| [ 0:11] ## Line: 31 	 	# restore to delay_dev tables for all devices
| [ 0:11] ## Line: 32 	 	aux restore_from_devtable "${DEVICES[@]}"
| [ 0:11] ## Line: 33 	 }
| [ 0:11] ## Line: 34 	 
| [ 0:11] ## Line: 35 	 # Delay legs so that rebuilding status characters can be read
| [ 0:11] ## Line: 36 	 for d in "${DEVICES[@]}"
| [ 0:11] ## Line: 37 	 do
| [ 0:11] ## Line: 38 	 	aux delay_dev "$d" 0 50 "$(get first_extent_sector "$d")"
| [ 0:11] ## Line: 39 	 done
| [ 0:11] ## Line: 40 	 
| [ 0:11] ## Line: 41 	 # rhbz 1064592
| [ 0:11] ## Line: 42 	 
| [ 0:11] ## Line: 43 	 ##############################################
| [ 0:11] ## Line: 44 	 # Create an 8-way striped raid10 with 4 mirror
| [ 0:11] ## Line: 45 	 # groups and rebuild selected PVs.
| [ 0:11] ## Line: 46 	 lvcreate --type raid10 -m 1 -i 4 -l 64 -n $lv1 $vg
| [ 0:11] ## Line: 47 	 _sync "AAAAAAAA"
| [ 0:11] ## Line: 48 	 
| [ 0:11] ## Line: 49 	 # Rebuild 1st and 2nd device would rebuild a
| [ 0:11] ## Line: 50 	 # whole mirror group and needs to be rejected.
| [ 0:11] ## Line: 51 	 not lvchange --yes --rebuild "$dev1" --rebuild "$dev2" $vg/$lv1
| [ 0:11] ## Line: 52 	 not check raid_leg_status $vg $lv1 "aaAAAAA"
| [ 0:11] ## Line: 53 	 _sync "AAAAAAAA"
| [ 0:11] ## Line: 54 	 
| [ 0:11] ## Line: 55 	 # Rebuild 1st and 3rd device from different mirror groups is fine.
| [ 0:11] ## Line: 56 	 lvchange --yes --rebuild "$dev1" --rebuild "$dev3" $vg/$lv1
| [ 0:11] ## Line: 57 	 [ $v1_9_0 -eq 1 ] && check raid_leg_status $vg $lv1 "aAaAAAAA"
| [ 0:11] ## Line: 58 	 _sync "AAAAAAAA"
| [ 0:11] ## Line: 59 	 
| [ 0:11] ## Line: 60 	 # Rebuild devices 1, 3, 6 from different mirror groups is fine.
