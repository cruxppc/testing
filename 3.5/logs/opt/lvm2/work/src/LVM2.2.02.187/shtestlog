| [ 2:15] ###########################################
| [ 2:15] # Must not be able to convert non-EX LVs in a cluster
| [ 2:15] ###########################################
| [ 2:15] if [ -e LOCAL_CLVMD ]; then
| [ 2:15] 	lvcreate -l 2 -n $lv1 $vg
| [ 2:15] 	not lvconvert -y --type raid1 -m 1 $vg/$lv1 \
| [ 2:15] 		--config 'global { mirror_segtype_default = "mirror" }'
| [ 2:15] 	lvremove -ff $vg
| [ 2:15] fi
| [ 2:15] #lvconvert-raid.sh:210+ '[' -e LOCAL_CLVMD ']'
| [ 2:15] 
| [ 2:15] ###########################################
| [ 2:15] # Mirror to RAID1 conversion
| [ 2:15] ###########################################
| [ 2:15] for i in 1 2 3 ; do
| [ 2:15] 	lvcreate -aey --type mirror -m $i -l 2 -n $lv1 $vg
| [ 2:15] 	aux wait_for_sync $vg $lv1
| [ 2:15] 	lvconvert -y --type raid1 $vg/$lv1
| [ 2:15] 	lvremove -ff $vg
| [ 2:15] done
