| [ 0:11] ## Line: 80 	 
| [ 0:11] ## Line: 81 	 ##############################################
| [ 0:11] ## Line: 82 	 # Create an 8-legged raid1 and rebuild selected PVs
| [ 0:11] ## Line: 83 	 lvremove --yes $vg/$lv1
| [ 0:11] ## Line: 84 	 lvcreate --yes --type raid1 -m 7 -l 2 -n $lv1 $vg
| [ 0:11] ## Line: 85 	 _sync "AAAAAAAA"
| [ 0:11] ## Line: 86 	 
| [ 0:11] ## Line: 87 	 # Rebuilding all raid1 legs needs to be rejected.
| [ 0:11] ## Line: 88 	 not lvchange --yes --rebuild "$dev1" --rebuild "$dev2" --rebuild "$dev3" --rebuild "$dev4" \
| [ 0:11] ## Line: 89 	 		   --rebuild "$dev5" --rebuild "$dev6" --rebuild "$dev7" --rebuild "$dev8" $vg/$lv1
| [ 0:11] ## Line: 90 	 not check raid_leg_status $vg $lv1 "aaaaaaaa"
| [ 0:11] ## Line: 91 	 _sync "AAAAAAAA"
| [ 0:11] ## Line: 92 	 
| [ 0:11] ## Line: 93 	 # Rebuilding all but the raid1 master leg is fine.
| [ 0:11] ## Line: 94 	 lvchange --yes --rebuild "$dev2" --rebuild "$dev3" --rebuild "$dev4" \
| [ 0:11] ## Line: 95 	 	       --rebuild "$dev5" --rebuild "$dev6" --rebuild "$dev7" --rebuild "$dev8" $vg/$lv1
| [ 0:11] ## Line: 96 	 [ $v1_9_0 -eq 1 ] && check raid_leg_status $vg $lv1 "Aaaaaaaa"
| [ 0:11] ## Line: 97 	 _sync "AAAAAAAA"
| [ 0:11] ## Line: 98 	 
| [ 0:11] ## Line: 99 	 # Rebuilding the raid1 master leg is fine.
| [ 0:11] ## Line: 100 	 lvchange --yes --rebuild "$dev1" $vg/$lv1
| [ 0:11] ## Line: 101 	 [ $v1_9_0 -eq 1 ] && check raid_leg_status $vg $lv1 "aAAAAAAA"
| [ 0:11] ## Line: 102 	 _sync "AAAAAAAA"
| [ 0:11] ## Line: 103 	 
| [ 0:11] ## Line: 104 	 # Rebuild legs on devices 2, 4, 6 and 8 is fine.
| [ 0:11] ## Line: 105 	 lvchange --yes --rebuild "$dev2" --rebuild "$dev4" --rebuild "$dev6" --rebuild "$dev8" $vg/$lv1
| [ 0:11] ## Line: 106 	 [ $v1_9_0 -eq 1 ] && check raid_leg_status $vg $lv1 "AaAaAaAa"
| [ 0:11] ## Line: 107 	 _sync "AAAAAAAA"
| [ 0:11] ## Line: 108 	 
| [ 0:11] ## Line: 109 	 ##############################################
| [ 0:11] ## Line: 110 	 # Create an 6-legged raid6 and rebuild selected PVs
| [ 0:11] ## Line: 111 	 lvremove --yes $vg/$lv1
| [ 0:11] ## Line: 112 	 lvcreate --yes --type raid6 -i 4 -l 2 -n $lv1 $vg
| [ 0:11] ## Line: 113 	 _sync "AAAAAA"
| [ 0:11] ## Line: 114 	 
| [ 0:11] ## Line: 115 	 # Rebuilding all raid6 stripes needs to be rejected.
| [ 0:11] ## Line: 116 	 not lvchange --yes --rebuild "$dev1" --rebuild "$dev2" --rebuild "$dev3" \
| [ 0:11] ## Line: 117 	 		   --rebuild "$dev4" --rebuild "$dev5" --rebuild "$dev6"  $vg/$lv1
| [ 0:11] ## Line: 118 	 not check raid_leg_status $vg $lv1 "aaaaaa"
| [ 0:11] ## Line: 119 	 _sync "AAAAAA"
| [ 0:11] ## Line: 120 	 
| [ 0:11] ## Line: 121 	 # Rebuilding more than 2 raid6 stripes needs to be rejected.
| [ 0:11] ## Line: 122 	 not lvchange --yes --rebuild "$dev2" --rebuild "$dev4" --rebuild "$dev6" $vg/$lv1
| [ 0:11] ## Line: 123 	 not check raid_leg_status $vg $lv1 "AaAaAa"
| [ 0:11] ## Line: 124 	 _sync "AAAAAA"
| [ 0:11] ## Line: 125 	 
| [ 0:11] ## Line: 126 	 # Rebuilding any 1 raid6 stripe is fine.
| [ 0:11] ## Line: 127 	 lvchange --yes --rebuild "$dev2" $vg/$lv1
| [ 0:11] ## Line: 128 	 [ $v1_9_0 -eq 1 ] && check raid_leg_status $vg $lv1 "AaAAAA"
| [ 0:11] ## Line: 129 	 _sync
| [ 0:11] ## Line: 130 	 
| [ 0:11] ## Line: 131 	 lvchange --yes --rebuild "$dev5"  $vg/$lv1
| [ 0:11] ## Line: 132 	 [ $v1_9_0 -eq 1 ] && check raid_leg_status $vg $lv1 "AAAAaA"
| [ 0:11] ## Line: 133 	 _sync "AAAAAA"
