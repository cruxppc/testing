| [ 2:53] ## Line: 21 	 get_image_pvs() {
| [ 2:53] ## Line: 22 	 	local d
| [ 2:53] ## Line: 23 	 	local images=()
| [ 2:53] ## Line: 24 	 
| [ 2:53] ## Line: 25 	 	images=( $(dmsetup ls | grep "${1}-${2}_.image_.*" | cut -f1 | sed -e s:-:/:) )
| [ 2:53] ## Line: 26 	 	lvs --noheadings -a -o devices "${images[@]}" | sed s/\(.\)//
| [ 2:53] ## Line: 27 	 }
| [ 2:53] ## Line: 28 	 
| [ 2:53] ## Line: 29 	 ########################################################
| [ 2:53] ## Line: 30 	 # MAIN
| [ 2:53] ## Line: 31 	 ########################################################
| [ 2:53] ## Line: 32 	 aux have_raid 1 3 0 || skip
| [ 2:53] ## Line: 33 	 
| [ 2:53] ## Line: 34 	 aux prepare_pvs 9
| [ 2:53] ## Line: 35 	 get_devs
| [ 2:53] ## Line: 36 	 
| [ 2:53] ## Line: 37 	 # vgcreate -s 256k "$vg" "${DEVICES[@]}"
| [ 2:53] ## Line: 38 	 vgcreate $SHARED -s 2m "$vg" "${DEVICES[@]}"
| [ 2:53] ## Line: 39 	 
| [ 2:53] ## Line: 40 	 ###########################################
| [ 2:53] ## Line: 41 	 # RAID1 convert tests
| [ 2:53] ## Line: 42 	 ###########################################
| [ 2:53] ## Line: 43 	 for under_snap in false true; do
| [ 2:53] ## Line: 44 	 for i in 1 2 3; do
| [ 2:53] ## Line: 45 	 	for j in 1 2 3; do
| [ 2:53] ## Line: 46 	 		if [ $i -eq 1 ]; then
| [ 2:53] ## Line: 47 	 			from="linear"
| [ 2:53] ## Line: 48 	 		else
| [ 2:53] ## Line: 49 	 			from="$i-way"
| [ 2:53] ## Line: 50 	 		fi
| [ 2:53] ## Line: 51 	 		if [ $j -eq 1 ]; then
| [ 2:53] ## Line: 52 	 			to="linear"
| [ 2:53] ## Line: 53 	 		else
| [ 2:53] ## Line: 54 	 			to="$j-way"
| [ 2:53] ## Line: 55 	 		fi
| [ 2:53] ## Line: 56 	 
| [ 2:53] ## Line: 57 	 		echo -n "Converting from $from to $to"
| [ 2:53] ## Line: 58 	 		if $under_snap; then
| [ 2:53] ## Line: 59 	 			echo -n " (while under a snapshot)"
| [ 2:53] ## Line: 60 	 		fi
| [ 2:53] ## Line: 61 	 		echo
| [ 2:53] ## Line: 62 	 
| [ 2:53] ## Line: 63 	 		if [ $i -eq 1 ]; then
| [ 2:53] ## Line: 64 	 			# Shouldn't be able to create with just 1 image
| [ 2:53] ## Line: 65 	 			not lvcreate --type raid1 -m 0 -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 66 	 
| [ 2:53] ## Line: 67 	 			lvcreate -aey -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 68 	 		else
| [ 2:53] ## Line: 69 	 			lvcreate --type raid1 -m $(( i - 1 )) -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 70 	 			aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 71 	 		fi
| [ 2:53] ## Line: 72 	 
| [ 2:53] ## Line: 73 	 		if $under_snap; then
| [ 2:53] ## Line: 74 	 			lvcreate -aey -s $vg/$lv1 -n snap -l 2
| [ 2:53] ## Line: 75 	 		fi
| [ 2:53] ## Line: 76 	 
| [ 2:53] ## Line: 77 	 		mirrors=$((j - 1))
| [ 2:53] ## Line: 78 	 		if [ $i -eq 1 ]
| [ 2:53] ## Line: 79 	 		then
| [ 2:53] ## Line: 80 	 			[ $mirrors -eq 0 ] && lvconvert -y -m $mirrors $vg/$lv1
| [ 2:53] ## Line: 81 	 		else
| [ 2:53] ## Line: 82 	 			if [ $mirrors -eq 0 ]
| [ 2:53] ## Line: 83 	 			then
| [ 2:53] ## Line: 84 	 				not lvconvert -m $mirrors $vg/$lv1
| [ 2:53] ## Line: 85 	 				lvconvert -y -m $mirrors $vg/$lv1
| [ 2:53] ## Line: 86 	 			else
| [ 2:53] ## Line: 87 	 				lvconvert -y -m $mirrors $vg/$lv1
| [ 2:53] ## Line: 88 	 			fi
| [ 2:53] ## Line: 89 	 		fi
| [ 2:53] ## Line: 90 	 
| [ 2:53] ## Line: 91 	 		# FIXME: ensure no residual devices
| [ 2:53] ## Line: 92 	 
| [ 2:53] ## Line: 93 	 		if [ $j -eq 1 ]; then
| [ 2:53] ## Line: 94 	 			check linear $vg $lv1
| [ 2:53] ## Line: 95 	 		fi
| [ 2:53] ## Line: 96 	 		lvremove -ff $vg
| [ 2:53] ## Line: 97 	 	done
| [ 2:53] ## Line: 98 	 done
| [ 2:53] ## Line: 99 	 done
| [ 2:53] ## Line: 100 	 
| [ 2:53] ## Line: 101 	 ##############################################
| [ 2:53] ## Line: 102 	 # RAID1 - shouldn't be able to add image
| [ 2:53] ## Line: 103 	 #         if created '--nosync', but should
| [ 2:53] ## Line: 104 	 #         be able to after 'lvchange --resync'
| [ 2:53] ## Line: 105 	 ##############################################
| [ 2:53] ## Line: 106 	 lvcreate --type raid1 -m 1 -l 2 -n $lv1 $vg --nosync
| [ 2:53] ## Line: 107 	 not lvconvert -m +1 $vg/$lv1
| [ 2:53] ## Line: 108 	 lvchange --resync -y $vg/$lv1
| [ 2:53] ## Line: 109 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 110 	 lvconvert -y -m +1 $vg/$lv1
| [ 2:53] ## Line: 111 	 lvremove -ff $vg
| [ 2:53] ## Line: 112 	 
| [ 2:53] ## Line: 113 	 # 3-way to 2-way convert while specifying devices
| [ 2:53] ## Line: 114 	 lvcreate --type raid1 -m 2 -l 2 -n $lv1 $vg "$dev1" "$dev2" "$dev3"
| [ 2:53] ## Line: 115 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 116 	 lvconvert -y -m 1 $vg/$lv1 "$dev2"
| [ 2:53] ## Line: 117 	 lvremove -ff $vg
| [ 2:53] ## Line: 118 	 
| [ 2:53] ## Line: 119 	 #
| [ 2:53] ## Line: 120 	 # FIXME: Add tests that specify particular devices to be removed
| [ 2:53] ## Line: 121 	 #
| [ 2:53] ## Line: 122 	 
| [ 2:53] ## Line: 123 	 ###########################################
| [ 2:53] ## Line: 124 	 # RAID1 split tests
| [ 2:53] ## Line: 125 	 ###########################################
| [ 2:53] ## Line: 126 	 # 3-way to 2-way/linear
| [ 2:53] ## Line: 127 	 lvcreate --type raid1 -m 2 -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 128 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 129 	 lvconvert --splitmirrors 1 -n $lv2 $vg/$lv1
| [ 2:53] ## Line: 130 	 check lv_exists $vg $lv1
| [ 2:53] ## Line: 131 	 check linear $vg $lv2
| [ 2:53] ## Line: 132 	 check active $vg $lv2
| [ 2:53] ## Line: 133 	 # FIXME: ensure no residual devices
| [ 2:53] ## Line: 134 	 lvremove -ff $vg
| [ 2:53] ## Line: 135 	 
| [ 2:53] ## Line: 136 	 # 2-way to linear/linear
| [ 2:53] ## Line: 137 	 lvcreate --type raid1 -m 1 -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 138 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 139 	 not lvconvert --splitmirrors 1 -n $lv2 $vg/$lv1
| [ 2:53] ## Line: 140 	 lvconvert --yes --splitmirrors 1 -n $lv2 $vg/$lv1
| [ 2:53] ## Line: 141 	 check linear $vg $lv1
| [ 2:53] ## Line: 142 	 check linear $vg $lv2
| [ 2:53] ## Line: 143 	 check active $vg $lv2
| [ 2:53] ## Line: 144 	 # FIXME: ensure no residual devices
| [ 2:53] ## Line: 145 	 lvremove -ff $vg
| [ 2:53] ## Line: 146 	 
| [ 2:53] ## Line: 147 	 # 4-way
| [ 2:53] ## Line: 148 	 lvcreate --type raid1 -m 4 -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 149 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 150 	 lvconvert --splitmirrors 1 --name $lv2 $vg/$lv1 "$dev2"
| [ 2:53] ## Line: 151 	 lvremove -ff $vg
| [ 2:53] ## Line: 152 	 
| [ 2:53] ## Line: 153 	 ###########################################
| [ 2:53] ## Line: 154 	 # RAID1 split + trackchanges / merge with content check
| [ 2:53] ## Line: 155 	 ###########################################
| [ 2:53] ## Line: 156 	 # 3-way to 2-way/linear
| [ 2:53] ## Line: 157 	 lvcreate --type raid1 -m 2 -l 1 -n $lv1 $vg
| [ 2:53] ## Line: 158 	 mkfs.ext4 "$DM_DEV_DIR/$vg/$lv1"
| [ 2:53] ## Line: 159 	 fsck.ext4 -fn "$DM_DEV_DIR/$vg/$lv1"
| [ 2:53] ## Line: 160 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 161 	 fsck.ext4 -fn "$DM_DEV_DIR/$vg/$lv1"
| [ 2:53] ## Line: 162 	 lvconvert --splitmirrors 1 --trackchanges $vg/$lv1
| [ 2:53] ## Line: 163 	 check lv_exists $vg $lv1
| [ 2:53] ## Line: 164 	 check linear $vg ${lv1}_rimage_2
| [ 2:53] ## Line: 165 	 fsck.ext4 -fn "$DM_DEV_DIR/mapper/$vg-${lv1}_rimage_2"
| [ 2:53] ## Line: 166 	 dd of="$DM_DEV_DIR/$vg/$lv1" if=/dev/zero bs=512 oflag=direct count="$(blockdev --getsz "$DM_DEV_DIR/$vg/$lv1")"
| [ 2:53] ## Line: 167 	 not fsck.ext4 -fn "$DM_DEV_DIR/$vg/$lv1"
| [ 2:53] ## Line: 168 	 fsck.ext4 -fn "$DM_DEV_DIR/mapper/$vg-${lv1}_rimage_2"
| [ 2:53] ## Line: 169 	 # FIXME: needed on tiny loop but not on real block backend ?
| [ 2:53] ## Line: 170 	 lvchange --refresh $vg/$lv1
| [ 2:53] ## Line: 171 	 lvconvert --merge $vg/${lv1}_rimage_2
| [ 2:53] ## Line: 172 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 173 	 lvconvert --splitmirrors 1 --trackchanges $vg/$lv1
| [ 2:53] ## Line: 174 	 not fsck.ext4 -fn "$DM_DEV_DIR/mapper/$vg-${lv1}_rimage_2"
| [ 2:53] ## Line: 175 	 # FIXME: ensure no residual devices
| [ 2:53] ## Line: 176 	 lvremove -ff $vg
| [ 2:53] ## Line: 177 	 
| [ 2:53] ## Line: 178 	 # Check split track changes gets rejected w/o -y on 2-legged raid1
| [ 2:53] ## Line: 179 	 lvcreate --type raid1 -m 1 -l 1 -n $lv1 $vg
| [ 2:53] ## Line: 180 	 mkfs.ext4 "$DM_DEV_DIR/$vg/$lv1"
| [ 2:53] ## Line: 181 	 fsck.ext4 -fn "$DM_DEV_DIR/$vg/$lv1"
| [ 2:53] ## Line: 182 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 183 	 fsck.ext4 -fn "$DM_DEV_DIR/$vg/$lv1"
| [ 2:53] ## Line: 184 	 not lvconvert --splitmirrors 1 --trackchanges $vg/$lv1
| [ 2:53] ## Line: 185 	 lvconvert --yes --splitmirrors 1 --trackchanges $vg/$lv1
| [ 2:53] ## Line: 186 	 # FIXME: ensure no residual devices
| [ 2:53] ## Line: 187 	 lvremove -ff $vg
| [ 2:53] ## Line: 188 	 
| [ 2:53] ## Line: 189 	 ###########################################
| [ 2:53] ## Line: 190 	 # Linear to RAID1 conversion ("raid1" default segtype)
| [ 2:53] ## Line: 191 	 ###########################################
| [ 2:53] ## Line: 192 	 lvcreate -aey -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 193 	 lvconvert -y -m 1 $vg/$lv1 \
| [ 2:53] ## Line: 194 	 	--config 'global { mirror_segtype_default = "raid1" }'
| [ 2:53] ## Line: 195 	 lvs --noheadings -o attr $vg/$lv1 | grep '^[[:space:]]*r'
| [ 2:53] ## Line: 196 	 lvremove -ff $vg
| [ 2:53] ## Line: 197 	 
| [ 2:53] ## Line: 198 	 ###########################################
| [ 2:53] ## Line: 199 	 # Linear to RAID1 conversion (override "mirror" default segtype)
| [ 2:53] ## Line: 200 	 ###########################################
| [ 2:53] ## Line: 201 	 lvcreate -aey -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 202 	 lvconvert --yes --type raid1 -m 1 $vg/$lv1 \
| [ 2:53] ## Line: 203 	 	--config 'global { mirror_segtype_default = "mirror" }'
| [ 2:53] ## Line: 204 	 lvs --noheadings -o attr $vg/$lv1 | grep '^[[:space:]]*r'
| [ 2:53] ## Line: 205 	 lvremove -ff $vg
| [ 2:53] ## Line: 206 	 
| [ 2:53] ## Line: 207 	 ###########################################
| [ 2:53] ## Line: 208 	 # Must not be able to convert non-EX LVs in a cluster
| [ 2:53] ## Line: 209 	 ###########################################
| [ 2:53] ## Line: 210 	 if [ -e LOCAL_CLVMD ]; then
| [ 2:53] ## Line: 211 	 	lvcreate -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 212 	 	not lvconvert -y --type raid1 -m 1 $vg/$lv1 \
| [ 2:53] ## Line: 213 	 		--config 'global { mirror_segtype_default = "mirror" }'
| [ 2:53] ## Line: 214 	 	lvremove -ff $vg
| [ 2:53] ## Line: 215 	 fi
| [ 2:53] ## Line: 216 	 
| [ 2:53] ## Line: 217 	 ###########################################
| [ 2:53] ## Line: 218 	 # Mirror to RAID1 conversion
| [ 2:53] ## Line: 219 	 ###########################################
| [ 2:53] ## Line: 220 	 for i in 1 2 3 ; do
| [ 2:53] ## Line: 221 	 	lvcreate -aey --type mirror -m $i -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 222 	 	aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 223 	 	lvconvert -y --type raid1 $vg/$lv1
| [ 2:53] ## Line: 224 	 	lvremove -ff $vg
| [ 2:53] ## Line: 225 	 done
| [ 2:53] ## Line: 226 	 
| [ 2:53] ## Line: 227 	 ###########################################
| [ 2:53] ## Line: 228 	 # Upconverted RAID1 should not allow loss of primary
| [ 2:53] ## Line: 229 	 #  - don't allow removal of primary while syncing
| [ 2:53] ## Line: 230 	 #  - DO allow removal of secondaries while syncing
| [ 2:53] ## Line: 231 	 ###########################################
| [ 2:53] ## Line: 232 	 aux delay_dev "$dev2" 0 100
| [ 2:53] ## Line: 233 	 lvcreate -aey -l 2 -n $lv1 $vg "$dev1"
| [ 2:53] ## Line: 234 	 lvconvert -y -m 1 $vg/$lv1 \
| [ 2:53] ## Line: 235 	 	--config 'global { mirror_segtype_default = "raid1" }' "$dev2"
| [ 2:53] ## Line: 236 	 lvs --noheadings -o attr $vg/$lv1 | grep '^[[:space:]]*r'
| [ 2:53] ## Line: 237 	 not lvconvert --yes -m 0 $vg/$lv1 "$dev1"
| [ 2:53] ## Line: 238 	 lvconvert --yes -m 0 $vg/$lv1 "$dev2"
| [ 2:53] ## Line: 239 	 aux enable_dev "$dev2"
| [ 2:53] ## Line: 240 	 lvremove -ff $vg
| [ 2:53] ## Line: 241 	 
| [ 2:53] ## Line: 242 	 ###########################################
| [ 2:53] ## Line: 243 	 # lvcreated RAID1 should allow all down-conversion
| [ 2:53] ## Line: 244 	 #  - DO allow removal of primary while syncing
| [ 2:53] ## Line: 245 	 #  - DO allow removal of secondaries while syncing
| [ 2:53] ## Line: 246 	 ###########################################
| [ 2:53] ## Line: 247 	 aux delay_dev "$dev2" 0 100
| [ 2:53] ## Line: 248 	 lvcreate --type raid1 -m 2 -aey -l 2 -n $lv1 $vg "$dev1" "$dev2" "$dev3"
| [ 2:53] ## Line: 249 	 lvconvert --yes -m 1 $vg/$lv1 "$dev3"
| [ 2:53] ## Line: 250 	 lvconvert --yes -m 0 $vg/$lv1 "$dev1"
| [ 2:53] ## Line: 251 	 aux enable_dev "$dev2"
| [ 2:53] ## Line: 252 	 lvremove -ff $vg
| [ 2:53] ## Line: 253 	 
| [ 2:53] ## Line: 254 	 ###########################################
| [ 2:53] ## Line: 255 	 # Converting from 2-way RAID1 to 3-way
| [ 2:53] ## Line: 256 	 #  - DO allow removal of one of primary sources
| [ 2:53] ## Line: 257 	 #  - Do not allow removal of all primary sources
| [ 2:53] ## Line: 258 	 ###########################################
| [ 2:53] ## Line: 259 	 lvcreate --type raid1 -m 1 -aey -l 2 -n $lv1 $vg "$dev1" "$dev2"
| [ 2:53] ## Line: 260 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 261 	 aux delay_dev "$dev3" 0 100
| [ 2:53] ## Line: 262 	 lvconvert --yes -m +1 $vg/$lv1 "$dev3"
| [ 2:53] ## Line: 263 	 # should allow 1st primary to be removed
| [ 2:53] ## Line: 264 	 lvconvert --yes -m -1 $vg/$lv1 "$dev1"
| [ 2:53] ## Line: 265 	 # should NOT allow last primary to be removed
| [ 2:53] ## Line: 266 	 not lvconvert --yes -m -1 $vg/$lv1 "$dev2"
| [ 2:53] ## Line: 267 	 # should allow non-primary to be removed
| [ 2:53] ## Line: 268 	 lvconvert --yes -m 0 $vg/$lv1 "$dev3"
| [ 2:53] ## Line: 269 	 aux enable_dev "$dev3"
| [ 2:53] ## Line: 270 	 lvremove -ff $vg
| [ 2:53] ## Line: 271 	 
| [ 2:53] ## Line: 272 	 ###########################################
| [ 2:53] ## Line: 273 	 # Converting from 2-way RAID1 to 3-way
| [ 2:53] ## Line: 274 	 #  - Should allow removal of two devices,
| [ 2:53] ## Line: 275 	 #    as long as they aren't both primary
| [ 2:53] ## Line: 276 	 ###########################################
| [ 2:53] ## Line: 277 	 lvcreate --type raid1 -m 1 -aey -l 2 -n $lv1 $vg "$dev1" "$dev2"
| [ 2:53] ## Line: 278 	 aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 279 	 aux delay_dev "$dev3" 0 100
| [ 2:53] ## Line: 280 	 lvconvert --yes -m +1 $vg/$lv1 "$dev3"
| [ 2:53] ## Line: 281 	 # should NOT allow both primaries to be removed
| [ 2:53] ## Line: 282 	 not lvconvert -m 0 $vg/$lv1 "$dev1" "$dev2"
| [ 2:53] ## Line: 283 	 # should allow primary + non-primary
| [ 2:53] ## Line: 284 	 lvconvert --yes -m 0 $vg/$lv1 "$dev1" "$dev3"
| [ 2:53] ## Line: 285 	 aux enable_dev "$dev3"
| [ 2:53] ## Line: 286 	 lvremove -ff $vg
| [ 2:53] ## Line: 287 	 
| [ 2:53] ## Line: 288 	 ###########################################
| [ 2:53] ## Line: 289 	 # Device Replacement Testing
| [ 2:53] ## Line: 290 	 ###########################################
| [ 2:53] ## Line: 291 	 # RAID1: Replace up to n-1 devices - trying different combinations
| [ 2:53] ## Line: 292 	 # Test for 2-way to 4-way RAID1 LVs
| [ 2:53] ## Line: 293 	 for i in {1..3}; do
| [ 2:53] ## Line: 294 	 	lvcreate --type raid1 -m "$i" -l 2 -n $lv1 $vg
| [ 2:53] ## Line: 295 	 
| [ 2:53] ## Line: 296 	 	for j in $(seq $(( i + 1 ))); do # The number of devs to replace at once
| [ 2:53] ## Line: 297 	 	for o in $(seq 0 $i); do        # The offset into the device list
| [ 2:53] ## Line: 298 	 		replace=()
| [ 2:53] ## Line: 299 	 
| [ 2:53] ## Line: 300 	 		devices=( $(get_image_pvs $vg $lv1) )
| [ 2:53] ## Line: 301 	 
| [ 2:53] ## Line: 302 	 		for k in $(seq "$j"); do
| [ 2:53] ## Line: 303 	 			index=$(( ( k +  o ) % ( i + 1 ) ))
| [ 2:53] ## Line: 304 	 			replace+=( "--replace" )
| [ 2:53] ## Line: 305 	 			replace+=( "${devices[$index]}" )
| [ 2:53] ## Line: 306 	 		done
| [ 2:53] ## Line: 307 	 		aux wait_for_sync $vg $lv1
| [ 2:53] ## Line: 308 	 
| [ 2:53] ## Line: 309 	 		if [ "$j" -ge $(( i + 1 )) ]; then
| [ 2:53] ## Line: 310 	 			# Can't replace all at once.
| [ 2:53] ## Line: 311 	 			not lvconvert "${replace[@]}" $vg/$lv1
| [ 2:53] ## Line: 312 	 		else
| [ 2:53] ## Line: 313 	 			lvconvert "${replace[@]}" $vg/$lv1
| [ 2:53] ## Line: 314 	 		fi
| [ 2:53] ## Line: 315 	 	done
| [ 2:53] ## Line: 316 	 	done
| [ 2:53] ## Line: 317 	 
| [ 2:53] ## Line: 318 	 	lvremove -ff $vg
| [ 2:53] ## Line: 319 	 done
