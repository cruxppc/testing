| [ 0:03] ###########################################
| [ 0:03] # RAID1 convert tests
| [ 0:03] ###########################################
| [ 0:03] for under_snap in false true; do
| [ 0:03] for i in 1 2 3; do
| [ 0:03] 	for j in 1 2 3; do
| [ 0:03] 		if [ $i -eq 1 ]; then
| [ 0:03] 			from="linear"
| [ 0:03] 		else
| [ 0:03] 			from="$i-way"
| [ 0:03] 		fi
| [ 0:03] 		if [ $j -eq 1 ]; then
| [ 0:03] 			to="linear"
| [ 0:03] 		else
| [ 0:03] 			to="$j-way"
| [ 0:03] 		fi
| [ 0:03] 
| [ 0:03] 		echo -n "Converting from $from to $to"
| [ 0:03] 		if $under_snap; then
| [ 0:03] 			echo -n " (while under a snapshot)"
| [ 0:03] 		fi
| [ 0:03] 		echo
| [ 0:03] 
| [ 0:03] 		if [ $i -eq 1 ]; then
| [ 0:03] 			# Shouldn't be able to create with just 1 image
| [ 0:03] 			not lvcreate --type raid1 -m 0 -l 2 -n $lv1 $vg
| [ 0:03] 
| [ 0:03] 			lvcreate -aey -l 2 -n $lv1 $vg
| [ 0:03] 		else
| [ 0:03] 			lvcreate --type raid1 -m $(( i - 1 )) -l 2 -n $lv1 $vg
| [ 0:03] 			aux wait_for_sync $vg $lv1
| [ 0:03] 		fi
| [ 0:03] 
| [ 0:03] 		if $under_snap; then
| [ 0:03] 			lvcreate -aey -s $vg/$lv1 -n snap -l 2
| [ 0:03] 		fi
| [ 0:03] 
| [ 0:03] 		mirrors=$((j - 1))
| [ 0:03] 		if [ $i -eq 1 ]
| [ 0:03] 		then
| [ 0:03] 			[ $mirrors -eq 0 ] && lvconvert -y -m $mirrors $vg/$lv1
| [ 0:03] 		else
| [ 0:03] 			if [ $mirrors -eq 0 ]
| [ 0:03] 			then
| [ 0:03] 				not lvconvert -m $mirrors $vg/$lv1
| [ 0:03] 				lvconvert -y -m $mirrors $vg/$lv1
| [ 0:03] 			else
| [ 0:03] 				lvconvert -y -m $mirrors $vg/$lv1
| [ 0:03] 			fi
| [ 0:03] 		fi
| [ 0:03] 
| [ 0:03] 		# FIXME: ensure no residual devices
| [ 0:03] 
| [ 0:03] 		if [ $j -eq 1 ]; then
| [ 0:03] 			check linear $vg $lv1
| [ 0:03] 		fi
| [ 0:03] 		lvremove -ff $vg
| [ 0:03] 	done
| [ 0:03] done
| [ 0:03] done
