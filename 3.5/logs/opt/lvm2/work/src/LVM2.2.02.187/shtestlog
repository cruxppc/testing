| [ 0:07] ## Line: 12 	 
| [ 0:07] ## Line: 13 	 
| [ 0:07] ## Line: 14 	 SKIP_WITH_LVMPOLLD=1
| [ 0:07] ## Line: 15 	 
| [ 0:07] ## Line: 16 	 . lib/inittest
| [ 0:07] ## Line: 17 	 
| [ 0:07] ## Line: 18 	 aux have_raid 1 3 0 || skip
| [ 0:07] ## Line: 19 	 
| [ 0:07] ## Line: 20 	 aux prepare_pvs 5
| [ 0:07] ## Line: 21 	 get_devs
| [ 0:07] ## Line: 22 	 
| [ 0:07] ## Line: 23 	 vgcreate $SHARED -s 256k "$vg" "${DEVICES[@]}"
| [ 0:07] ## Line: 24 	 
| [ 0:07] ## Line: 25 	 # Start with linear on 2 PV and ensure that converting to
| [ 0:07] ## Line: 26 	 # RAID is not allowed to reuse PVs for different images.  (Bug 1113180)
| [ 0:07] ## Line: 27 	 lvcreate -aey -l 4 -n $lv1 $vg "$dev1:0-1" "$dev2:0-1"
| [ 0:07] ## Line: 28 	 not lvconvert -y --type raid1 -m 1 $vg/$lv1 "$dev1" "$dev2"
| [ 0:07] ## Line: 29 	 not lvconvert -y --type raid1 -m 1 $vg/$lv1 "$dev1" "$dev3:0-2"
| [ 0:07] ## Line: 30 	 lvconvert -y --type raid1 -m 1 $vg/$lv1 "$dev3"
| [ 0:07] ## Line: 31 	 not lvconvert -m 0 $vg/$lv1
| [ 0:07] ## Line: 32 	 lvconvert -y -m 0 $vg/$lv1
| [ 0:07] ## Line: 33 	 # RAID conversions are not honoring allocation policy!
| [ 0:07] ## Line: 34 	 # lvconvert -y --type raid1 -m 1 --alloc anywhere $vg/$lv1 "$dev1" "$dev2"
| [ 0:07] ## Line: 35 	 lvremove -ff $vg
| [ 0:07] ## Line: 36 	 
| [ 0:07] ## Line: 37 	 
| [ 0:07] ## Line: 38 	 # Setup 2-way RAID1 LV, spread across 4 devices.
| [ 0:07] ## Line: 39 	 # For each image:
| [ 0:07] ## Line: 40 	 #  - metadata LV + 1 image extent (2 total extents) on one PV
| [ 0:07] ## Line: 41 	 #  - 2 image extents on the other PV
| [ 0:07] ## Line: 42 	 # Then attempt allocation of another image from 2 extents on
| [ 0:07] ## Line: 43 	 # a 5th PV and the remainder of the rest of already used PVs.
| [ 0:07] ## Line: 44 	 #
| [ 0:07] ## Line: 45 	 # This should fail because there is insufficient space on the
| [ 0:07] ## Line: 46 	 # non-parallel PV (i.e. there is not enough space for the image
| [ 0:07] ## Line: 47 	 # if it doesn't share a PV with another image).
| [ 0:07] ## Line: 48 	 lvcreate --type raid1 -m 1 -l 3 -n $lv1 $vg \
| [ 0:07] ## Line: 49 	     "$dev1:0-1" "$dev2:0-1" "$dev3:0-1" "$dev4:0-1"
| [ 0:07] ## Line: 50 	 aux wait_for_sync $vg $lv1
| [ 0:07] ## Line: 51 	 # Should not be enough non-overlapping space.
| [ 0:07] ## Line: 52 	 not lvconvert -m +1 $vg/$lv1 \
| [ 0:07] ## Line: 53 	     "$dev5:0-1" "$dev1" "$dev2" "$dev3" "$dev4"
| [ 0:07] ## Line: 54 	 lvconvert -y -m +1 $vg/$lv1 "$dev5"
| [ 0:07] ## Line: 55 	 not lvconvert -m 0 $vg/$lv1
| [ 0:07] ## Line: 56 	 lvconvert -y -m 0 $vg/$lv1
| [ 0:07] ## Line: 57 	 # Should work due to '--alloc anywhere'
| [ 0:07] ## Line: 58 	 # RAID conversion not honoring allocation policy!
| [ 0:07] ## Line: 59 	 #lvconvert -y -m +1 --alloc anywhere $vg/$lv1 \
| [ 0:07] ## Line: 60 	 #    "$dev5:0-1" "$dev1" "$dev2" "$dev3" "$dev4"
| [ 0:07] ## Line: 61 	 lvremove -ff $vg
| [ 0:07] ## Line: 62 	 
| [ 0:07] ## Line: 63 	 
| [ 0:07] ## Line: 64 	 # Setup 2-way RAID1 LV, spread across 4 devices
| [ 0:07] ## Line: 65 	 #  - metadata LV + 1 image extent (2 total extents) on one PV
| [ 0:07] ## Line: 66 	 #  - 2 image extents on the other PV
| [ 0:07] ## Line: 67 	 # Kill one PV.  There should be enough space on the remaining
| [ 0:07] ## Line: 68 	 # PV for that image to reallocate the entire image there and
| [ 0:07] ## Line: 69 	 # still maintain redundancy.
| [ 0:07] ## Line: 70 	 lvcreate --type raid1 -m 1 -l 3 -n $lv1 $vg \
| [ 0:07] ## Line: 71 	     "$dev1:0-1" "$dev2:0-1" "$dev3:0-1" "$dev4:0-1"
| [ 0:07] ## Line: 72 	 aux wait_for_sync $vg $lv1
| [ 0:07] ## Line: 73 	 aux disable_dev "$dev1"
| [ 0:07] ## Line: 74 	 lvconvert -y --repair $vg/$lv1 "$dev2" "$dev3" "$dev4"
| [ 0:07] ## Line: 75 	 #FIXME: ensure non-overlapping images (they should not share PVs)
| [ 0:07] ## Line: 76 	 aux enable_dev "$dev1"
| [ 0:07] ## Line: 77 	 lvremove -ff $vg
| [ 0:07] ## Line: 78 	 
| [ 0:07] ## Line: 79 	 vgremove -ff $vg
| [ 0:07] aux teardown
| [ 0:07] #lvconvert-raid-allocation.sh:1+ aux teardown
